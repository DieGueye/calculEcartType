   "source": [
    "def ecartType(a: Array[Double]): Double = {\n",
    "  val moyenne = a.sum / a.length\n",
    "  math.sqrt(a.map(x => math.pow(x - moyenne, 2)).sum / a.length)\n",
    "}"
   ]
